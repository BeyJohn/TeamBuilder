<Window x:Class="TeamBuilder.DisplayTeams"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TeamBuilder"
        mc:Ignorable="d"
        Title="DisplayTeams" Height="400" Width="600" WindowStartupLocation="CenterScreen">
    <Grid>
        <Button Content="Next Team" HorizontalAlignment="Left" VerticalAlignment="Top" Width="125" Margin="225,180,0,0" Height="40" Click="NextTeam" />
        <Label x:Name="lSecond" Content="" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="75,180,0,0" Height="40" Width="125" />
        <Label x:Name="lFirst" Content="" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="225,115,0,0" Height="40" Width="125" />
        <Label x:Name="lThird" Content="" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="375,180,0,0" Height="40" Width="125" />
        <Button Content="Back" HorizontalAlignment="Left" VerticalAlignment="Top" Width="125" Margin="375,240,0,0" Height="40" Click="Back" />

    </Grid>
</Window>